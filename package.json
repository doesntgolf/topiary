    "@types/mithril": "^1.1.12",
    "@types/mustache": "^0.8.31",
    "jest": "^23.2.0",
    "prettier": "^1.10.2",
    "rollup": "^0.63.5",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-pathmodify": "^1.0.1",
